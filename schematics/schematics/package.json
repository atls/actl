{
  "name": "@atls/schematics",
  "version": "1.0.1",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./collection": "./dist/collection.json"
  },
  "files": [
    "collection.json",
    "dist"
  ],
  "scripts": {
    "build": " yarn node ./src/scripts/cjs-build.script.ts",
    "prepack": "yarn run build && yarn node ./src/scripts/prepack.script.ts",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@angular-devkit/core": "19.1.5",
    "@angular-devkit/schematics": "19.1.5",
    "js-yaml": "4.1.0",
    "strip-json-comments": "3.1.1"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "copy-dir": "1.3.0",
    "esbuild": "0.24.2",
    "globby": "13.2.2"
  },
  "peerDependencies": {
    "@atls/code-runtime": "*"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "typings": "dist/index.d.ts"
  },
  "schematics": "./dist/collection.json",
  "typecheckSkipLibCheck": true
}
