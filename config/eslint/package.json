{
  "name": "@atls/config-eslint",
  "version": "0.0.8",
  "license": "BSD-3-Clause",
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./eslintrc": {
      "import": "./dist/.eslintrc.js",
      "require": "./dist/.eslintrc.js"
    }
  },
  "scripts": {
    "build": "yarn library build && node -e 'const fs = require(\"fs\"); const config = require(\"./dist/.eslintrc.js\"); fs.writeFileSync(\"./dist/.eslintrc.json\", JSON.stringify(config, null, 2))'",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/config-prettier": "workspace:*",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.17.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "prettier": "2.5.1",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@types/eslint": "8.4.2",
    "@types/node": "18.18.13",
    "typescript": "4.6.4"
  },
  "publishConfig": {
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
