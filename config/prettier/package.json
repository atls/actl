{
  "name": "@atls/config-prettier",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build && node -e 'const fs = require(\"fs\"); const config = require(\"./dist/index.js\"); fs.writeFileSync(\"./dist/.prettierrc\", JSON.stringify(config.default, null, 2))'",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "prettier": "3.1.1"
  },
  "devDependencies": {
    "@types/node": "20.11.20"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "typings": "dist/index.d.ts"
  }
}
