{
  "name": "@atls/yarn-pack-utils",
  "version": "1.0.0",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "semver": "7.6.3"
  },
  "devDependencies": {
    "@types/semver": "7.5.8",
    "@yarnpkg/cli": "4.4.1",
    "@yarnpkg/core": "4.1.2",
    "@yarnpkg/fslib": "3.1.0",
    "@yarnpkg/plugin-pack": "4.0.0",
    "@yarnpkg/plugin-patch": "4.0.1",
    "tar-stream": "3.1.7"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
