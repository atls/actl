{
  "name": "@atls/yarn-plugin-renderer",
  "version": "1.0.0",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "builder build plugin",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "clipanion": "4.0.0-rc.3",
    "cross-spawn": "7.0.3",
    "localtunnel": "2.0.2"
  },
  "devDependencies": {
    "@atls/yarn-run-utils": "workspace:*",
    "@atls/yarn-test-utils": "workspace:*",
    "@jest/globals": "29.7.0",
    "@types/cross-spawn": "6.0.6",
    "@types/localtunnel": "2.0.4",
    "@yarnpkg/builder": "4.2.0",
    "@yarnpkg/cli": "4.4.1",
    "@yarnpkg/core": "4.1.2",
    "@yarnpkg/fslib": "3.1.0"
  },
  "peerDependencies": {
    "@yarnpkg/cli": "^4",
    "@yarnpkg/core": "^4"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  }
}
