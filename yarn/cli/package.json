    "@yarnpkg/plugin-workspace-tools": "^3.1.6",
    "@yarnpkg/shell": "^3.2.5",
    "clipanion": "^3.2.0",
    "semver": "^7.3.8",
    "typanion": "^3.12.1",
    "yup": "^1.0.2"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  },
  "@yarnpkg/builder": {
    "bundles": {
      "standard": [
        "@atls/yarn-plugin-badges",
