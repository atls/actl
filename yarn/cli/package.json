    "@yarnpkg/shell": "4.0.0",
    "ci-info": "4.0.0",
    "clipanion": "4.0.0-rc.2",
    "semver": "7.5.4",
    "typanion": "3.14.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "super-resolve": "^1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
