        "@yarnpkg/plugin-github",
        "@yarnpkg/plugin-http",
        "@yarnpkg/plugin-init",
        "@yarnpkg/plugin-link",
        "@yarnpkg/plugin-nm",
        "@yarnpkg/plugin-npm",
        "@yarnpkg/plugin-npm-cli",
        "@yarnpkg/plugin-pack",
        "@yarnpkg/plugin-patch",
