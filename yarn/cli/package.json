{
  "name": "@atls/yarn-cli",
  "version": "0.0.1",
  "private": true,
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "builder build bundle && mv bundles/yarn.js bundles/yarn.cjs",
    "fix": "./fix.sh",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
