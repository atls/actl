{
  "name": "@atls/code-schematics-worker",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn run build:worker && yarn run build:source",
    "build:source": "yarn library build",
    "build:worker": "rollup -c",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/code-schematics": "workspace:*",
    "@atls/code-worker-utils": "workspace:*"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "18.2.2",
    "@rollup/plugin-commonjs": "25.0.8",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "5.0.5",
    "@types/node": "22.5.1",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.19.9",
    "rollup": "3.29.4",
    "rollup-plugin-esbuild": "5.0.0",
    "rollup-plugin-terser": "7.0.2",
    "rxjs": "7.8.1"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "typings": "dist/index.d.ts"
  }
}
