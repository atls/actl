{
  "name": "@atls/code-pack",
  "version": "0.0.19",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@actions/github": "6.0.0",
    "@iarna/toml": "3.0.0",
    "@yarnpkg/core": "4.0.3",
    "@yarnpkg/fslib": "3.0.2"
  },
  "devDependencies": {
    "@types/node": "20"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "typings": "dist/index.d.ts"
  }
}
