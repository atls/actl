{
  "name": "@atls/code-lint",
  "version": "0.0.32",
  "license": "BSD-3-Clause",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn library build",
    "prepack": "yarn run build",
    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "@atls/code-runtime": "workspace:*",
    "globby": "^13.1.3",
    "ignore": "^5.2.4"
  },
  "devDependencies": {
    "@types/node": "20"
  },
  "peerDependencies": {
    "react": "*",
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "typings": "dist/index.d.ts"
  }
}
