{
  "name": "@atlantis-lab/actl-check",
  "repository": "git@github.com:Atlantis-Lab/actl.git",
  "version": "0.4.17",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "oclif": {
    "bin": "actl",
    "commands": "./lib/commands",
    "topics": {
      "check": {
        "description": "Github checks"
      }
    }
  },
  "scripts": {
    "clean": "rm -r -f lib oclif.manifest.json",
    "prepublishOnly": "yarn run build",
    "build": "tsc && oclif-dev manifest readme",
    "prepack": "pubflow store",
    "postpack": "pubflow restore"
  },
  "dependencies": {
    "@actions/core": "1.2.0",
    "@actions/github": "1.1.0",
    "@atlantis-lab/config": "^0.1.8",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-help": "2.2.2",
    "@octokit/graphql": "4.3.1",
    "@types/jest": "24.9.1",
    "commitlint-format-json": "1.1.0",
    "eslint": "6.7.2",
    "execa": "3.4.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@monstrs/publish-flow": "^0.1.3",
    "@oclif/dev-cli": "^1",
    "@types/eslint": "6.1.3",
    "@types/execa": "2.0.0",
    "@types/node": "14.0.14",
    "typescript": "3.9.6"
  }
}
